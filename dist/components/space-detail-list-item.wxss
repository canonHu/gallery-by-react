.space-detail-blank-style {
  height: 9px;
  background-color: #EDF0F5;
}
.space-detail-item-view {
  background-color: white;
  display: flex;
  flex-direction: column;
  padding-top: 12px;
}
.space-detail-content-view {
  padding-left: 15px;
  padding-right: 15px;
  padding-bottom: 10px;
  display: flex;
  flex-direction: column;
}
.space-detail-content-title {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.space-detail-content-title-view {
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-right: 57px;
}
.space-detail-content-title-text {
  font-size: 17px;
  color: #333333;
  font-weight: bold;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.space-detail-content-title-dots {
  display: flex;
  justify-content: center;
  padding-top: 5px;
  padding-bottom: 5px;
}
.space-detail-quote-tip-view {
  position: absolute;
  right: -42px;
  margin-left: 5px;
  width: 36px;
  height: 17px;
  border-radius: 2px;
  border: 0.5px solid #65ABFC;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.space-detail-quote-tip {
  font-size: 12px;
  color: #65ABFC;
}
.space-detail-content-content-text {
  font-size: 14px;
  color: #777777;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.space-detail-user-info-view {
  margin-top: 8px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.space-detail-user-info {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.space-detail-user-text-info {
  display: flex;
  flex-direction: column;
  margin-left: 9px;
}
.space-detail-user-name {
  font-size: 13px;
  color: #666666;
}
.space-detail-date-time {
  font-size: 13px;
  color: #999999;
}
.space-detail-content-pv-view {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.space-detail-content-pv-count-text {
  padding-left: 1px;
  padding-right: 2px;
  font-size: 13px;
  color: #4A4C5B;
}
.space-detail-content-pv-text {
  margin-top: -6px;
  font-size: 13px;
  color: #777777;
}
.space-detail-img-total {
  position: absolute;
  bottom: 0px;
  right: 0px;
  height: 22px;
  padding-left: 8px;
  padding-right: 8px;
  background-color: rgba(0, 0, 0, 0.5);
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.space-detail-img-total-text {
  font-size: 13px;
  color: #ffffff;
}
