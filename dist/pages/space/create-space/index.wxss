page {
  background-color: #EDF0F5;
}
.current-back {
  background-color: white;
}
.cell-style {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-left: 16px;
  padding-top: 13px;
  padding-bottom: 13px;
  padding-left: -1px;
  padding-right: 15px;
  border-bottom: 0.5px solid rgba(0, 0, 0, 0.1);
}
.cell-title-style {
  font-size: 16px;
  color: #333333;
}
.current-permissions-style {
  font-size: 16px;
  color: #999999;
}
.tag-list {
  padding-top: 9px;
  display: flex;
  padding-left: 15px;
  padding-right: 6px;
  flex-wrap: wrap;
}
.poi-button {
  background-color: #F2F4F7;
  border-radius: 100px;
  padding-top: 7.5px;
  padding-bottom: 7.5px;
  padding-right: 16px;
  padding-left: 15px;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-right: 9px;
  margin-bottom: 9px;
}
.poi-text {
  font-size: 14px;
  color: #333333;
}
.delete-button {
  width: 12px;
  height: 12px;
  margin-left: 4px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.complete-view {
  margin-top: 40px;
  background-color: #FF7733;
  margin-left: 20px;
  margin-right: 20px;
  border-radius: 100px;
  padding-top: 11px;
  padding-bottom: 11px;
  display: flex;
  flex-direction: row;
  justify-content: center;
  box-shadow: -1px 6px 13px 0 rgba(255, 111, 10, 0.3);
}
.complete-text {
  font-size: 16px;
  color: #FFFFFF;
  font-weight: bold;
}
