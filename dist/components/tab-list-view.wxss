.tab-list-view {
  background-color: #EDF0F5;
  width: 100%;
  padding-top: 14px;
  padding-left: 5px;
  padding-right: 14px;
  display: flex;
  flex-wrap: wrap;
}
.tab-list-button {
  border-radius: 2px;
  margin-bottom: 12px;
  margin-left: 9px;
  padding-left: 25px;
  padding-right: 25px;
  height: 30px;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.tab-list-view-tab-text {
  font-size: 14px;
}
