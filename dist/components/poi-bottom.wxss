.line {
  flex-wrap: wrap;
  display: flex;
  padding-left: 14px;
  padding-right: 5px;
}
.line .label {
  height: 20px;
  display: flex;
  flex-direction: row;
  margin-right: 9px;
  border-radius: 2px;
  margin-bottom: 9px;
  background-color: #edf0f5;
  align-items: center;
}
.line .icon {
  width: 11px;
  height: 12px;
  margin-left: 5px;
  margin-right: 1px;
}
.line .text {
  display: flex;
  font-size: 12px;
  color: #666666;
  margin-right: 5px;
}
