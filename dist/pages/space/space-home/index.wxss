@import "./../../../components/space-item.wxss";
.header-style {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-bottom: 39px;
  padding-top: 42px;
}
.header-info-other-style {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 10px;
}
.header-info-first-style {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.space-use-text {
  font-size: 16px;
  color: #999999;
}
.space-use-text-normal {
  margin-top: -5px;
  font-size: 14px;
  color: #999999;
}
.space-use-person-text-normal {
  font-weight: bold;
  font-size: 44px;
  color: #4A4C5B;
}
.space-use-person-text {
  font-size: 18px;
  color: #4A4A4A;
  font-weight: bold;
}
.space-button {
  margin-top: 18px;
  padding: 9px 40px 10px 41px;
  border-radius: 100px;
  background-color: #FF7733;
  color: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
  box-shadow: -1px 6px 13px 0 rgba(255, 111, 10, 0.3);
}
.space-create-text {
  font-size: 18px;
}
.title-style {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding-left: 20px;
  padding-right: 20px;
  margin-bottom: 13px;
}
.title-text-style {
  font-size: 16px;
  color: #333333;
}
.title-more-style {
  font-size: 14px;
  color: #999999;
}
.more-arrow-button {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.space-header-title-style {
  display: flex;
  justify-content: center;
  align-items: center;
  padding-bottom: 17px;
}
.space-title-style {
  font-size: 18px;
  font-weight: bold;
}
.space-title-text-style {
  margin-left: 6px;
  margin-right: 6px;
}
.space-short-line {
  height: 0px;
  width: 36px;
  border-top: 0.5px solid #E9E9E9;
}
.space-long-line {
  height: 0px;
  margin-left: 12px;
  margin-right: 16px;
  border-top: 0.5px solid #E9E9E9;
}
.space-dynamic-view {
  margin-bottom: 8px;
  padding-top: 6px;
  padding-left: 22px;
  padding-right: 15px;
  padding-bottom: 21px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.space-dynamic-details-title {
  display: flex;
  align-items: center;
}
.space-dynamic-details-title-text {
  font-size: 14px;
  color: #333333;
}
.space-dynamic-details-content {
  margin-right: 1px;
  padding-left: 20px;
  margin-top: 3px;
  height: 20px;
  width: 250px;
}
.space-dynamic-details-content-text {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  width: 224px;
  font-size: 14px;
  color: #999999;
}
.space-dynamic-details-content-username {
  font-size: 14px;
  color: #666666;
  margin-right: 7px;
}
.space-dynamic-list-entrance {
  display: flex;
  align-items: center;
}
.space-dynamic-list-total-count {
  font-size: 14px;
  color: #7BB7FD;
  margin-right: 6px;
}
.more-view {
  background-color: #ffffff;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin-top: 11px;
  margin-bottom: 26px;
}
.more-button {
  width: 100px;
  height: 30px;
  border-radius: 100px;
  border: 0.5px solid rgba(0, 0, 0, 0.1);
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.more-text {
  font-size: 13px;
  color: #666666;
}
.list {
  display: flex;
  flex-direction: row;
}
.center-list {
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.history-button {
  position: fixed;
  bottom: 22px;
  right: 10px;
}
