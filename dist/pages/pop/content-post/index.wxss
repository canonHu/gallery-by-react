.time {
  display: flex;
  margin-top: 15px;
  margin-left: 33.5px;
  flex-direction: row;
  align-items: center;
}
.time .day {
  font-size: 48px;
  line-height: 67px;
  color: #333333;
}
.time .small-text {
  font-size: 14px;
  line-height: 20px;
  color: #999999;
}
.bottom-content {
  position: fixed;
  bottom: 58px;
  width: 100%;
  align-items: center;
  justify-self: center;
}
.list {
  position: relative;
  width: 100%;
  flex-direction: row;
  display: flex;
  justify-content: space-around;
}
.item-container {
  flex-direction: column;
  align-items: center;
  display: flex;
}
.list .image {
  width: 70px;
  height: 70px;
}
.list .text {
  font-size: 14px;
  line-height: 19px;
  margin-top: 9px;
  color: #333333;
  position: relative;
}
.close-image {
  width: 30px;
  height: 30px;
  margin-top: 131px;
}
